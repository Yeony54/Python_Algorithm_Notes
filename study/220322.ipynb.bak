    "# 오답 [1, 5, 6, 10] 중에서 오답이 있었던것 같다."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3d5ec82a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 최종코드\n",
    "def solution(a, b):\n",
    "\n",
    "    # 지수를 4로 나눠서 최소의 계산을 할 수 있도록함\n",
    "    b = b%4        \n",
    "    if b == 0 : b = 4\n",
    "    \n",
    "    # a를 최소로 만들어줌\n",
    "    num = int(str(a)[-1])\n",
    "    if a in [1, 5, 6, 0]: return (a if a!=0 else 10)\n",
    "\n",
    "    # range(1, b) 만큼 지수계산을 함\n",
    "    # 지수가 1일 때에는 지수계산을 하지 않음\n",
    "    for i in range(1,b):\n",
    "        \n",
    "        # 현재 숫자(num)에 a를 곱하여 마지막 숫자를 int로 바꿔서 num에 저장\n",
    "        num = int(str(num*a)[-1])\n",
    "    \n",
    "    # 0번째 컴퓨터는 10번째 컴퓨터를 의미한다.\n",
    "    return (10 if num==0 else num)"
   ]
